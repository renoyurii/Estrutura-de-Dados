package ordenacao;

class Produto {
    int codigo;
    String nome;
    double valor;

    public Produto(int codigo, String nome, double valor) {
        this.codigo = codigo;
        this.nome = nome;
        this.valor = valor;
    }

    @Override
    public String toString() {
        return "CÃ³digo: " + codigo + ", Nome: " + nome + ", Valor: R$ " + valor;
    }
}
